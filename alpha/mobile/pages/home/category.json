{
    "type": "container",
    "sections": {
        
        "title": {
            "type": "text",
            "text": "{{category.title}}",
            "style": {
                "textAlign": "center",
                "fontFamily": "Noto Sans",
                "fontSize": "13px",
                "fontWeight": "400",
                "color": "var(--primary-text-color)"
                
            }
        }
    },
    "order": [
        
