{
    "type": "container",
    "sections": {
        "image": {
            "type": "product-image",
            "url": "{{category.image}}",
            "style": {
                "height": "70px",
                "width": "70px",
                "borderRadius": "70px"
            }
        },
        "container1": {
            "type": "container",
            "sections": {
                "heading1": {
                    "type": "text",
                    "text": "{{category.title}}"
                }
            },
            "order": ["heading1"],
            "style": {
                "marginLeft": "0px",
                "flexDirection": "column",
                "textAlign": "center",
                "display": "flex",
                "alignItems": "center",
                "justifyContent": "center"
            }
        }
    },
    "order": ["container1"],
    "style": {
        "display": "flex",
        "minWidth": "120px",
        "alignItems": "center",
        "justifyContent": "center",
        "marginLeft": "5px",
        "marginTop": "0px",
        "marginBottom": "0px",
        "marginRight": "0px",
        "flexDirection": "row",
        "background": "#00bdff00"
    }
}
