{
    "type": "container",
    "className": "category-container",
    "sections": {
        "image": {
            "type": "product-image",
            "url": "{{category.image}}",
            "style": {
                "height": "70px",
                "width": "70px",
                "borderRadius": "70px"
            }
        },
        "container1": {
            "type": "container",
            "className": "category-title-container",
            "sections": {
                "heading1": {
                    "type": "text",
                    "text": "{{category.title}}",
                    "className": "category-title"
                }
            },
            "order": [
                "heading1"
            ]
        },
        "slash": {
            "type": "text",
            "text": " / ",
            "style": {
                "width": "20%",
                "textAlign": "center",
                "fontFamily": "Noto Sans",
                "fontSize": "14px",
                "fontWeight": "500",
                "color": "var(--placeholder-text-color)"
            }
        }
    },
    "order": [
        "container1"
    ]
}